{
  "name": "move37-polling-backend",
  "version": "1.0.0",
  "description": "Real-time polling application backend with Node.js, Express, PostgreSQL, Prisma, and WebSockets",
  "keywords": [
    "polling",
    "real-time",
    "websockets",
    "express",
    "prisma",
    "postgresql"
  ],
  "license": "MIT",
  "author": "Move37 Ventures Challenge",
  "main": "src/server.js",
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "node src/seed.js",
    "db:studio": "prisma studio",
    "dev": "nodemon src/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "frontend": "next dev -p 3001",
    "frontend:build": "next build",
    "frontend:start": "next start -p 3001",
    "start": "node src/server.js"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "geist": "^1.5.1",
    "helmet": "^8.1.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.454.0",
    "next": "^15.2.4",
    "prisma": "^5.22.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.18.4",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  }
}
